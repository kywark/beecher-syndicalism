      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "\"[syndicalism] basically means that anyone who tries to work for justice in our society is doing something against the law - this is ironic, this is a mockery.\"",
        "format": "text/plain"
      },
      "target": {
        "source": "https://kywark.github.io/beecher-syndicalism/beecher-criminal-syndicalism-case-mccomb-mississippi-side-1-/canvas-1/canvas",
        "selector": {
