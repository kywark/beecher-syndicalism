      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "\"syndicalism. some of us, you know, stumble over (when we gotta?) pronounce the word\"",
        "format": "text/plain"
      },
      "target": {
        "source": "https://kywark.github.io/beecher-syndicalism/beecher-criminal-syndicalism-case-mccomb-mississippi-side-1-/canvas-1/canvas",
